prompt 0
timeout 20
default bigtera60/vesamenu.c32

LABEL Install Ubuntu 12.04 (Ubuntu 12.04)
    menu label Ubuntu 12.04 (Ubuntu 12.04)
    kernel bigtera60/vmlinuz
    append initrd=bigtera60/initrd.aoecdrom.gz root=/dev/ram0 locale=en_US console-setup/ask_detect=false vga=788 keyboard-configuration/layoutcode=us hostname=hmos138 interface=eth0 url=http://10.80.3.126/seed_controller_6.1/ubuntu-ezs3-ctrl-wtlicense.seed aoecdrom=e12.4 quiet --

LABEL Boot From Local Disk
  localboot 0
  TEXT HELP
  Boot to the local hard disk
  ENDTEXT
